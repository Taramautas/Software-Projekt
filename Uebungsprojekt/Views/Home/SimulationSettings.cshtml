@{
    ViewData["Title"] = "Simulation";
    Layout = "_PlanerLayout";
}
<h1 class="page-title">@ViewData["Title"]</h1>
<br />
<form>
    <div><h3><u>Booking Distribution</u></h3></div>
    <br />
    <div class="form-group row">
        <label for=" " class="col-sm-2 col-form-label">Peak Time</label>
        <div class="col-sm-10">
            <input type=" " class="form-control" id=" " placeholder="Please enter a valid time">
        </div>
    </div>
    <div class="form-group row">
        <label for=" " class="col-sm-2 col-form-label">Min</label>
        <div class="col-sm-10">
            <input type=" " class="form-control" id=" " placeholder="Please enter the minimum number of Bookings">
        </div>
    </div>
    <div class="form-group row">
        <label for=" " class="col-sm-2 col-form-label">Max</label>
        <div class="col-sm-10">
            <input type=" " class="form-control" id=" " placeholder="Please enter the maximum number of Bookings">
        </div>
    </div>
    <div class="form-group row">
        <label for=" " class="col-sm-2 col-form-label">Spread</label>
        <div class="col-sm-10">
            <input type=" " class="form-control" id=" " placeholder="Please enter the amount of peanut butter you want on your toast <3">
        </div>
    </div>
    <div><h3><u>Tick Rate</u></h3></div>
    <div class="form-group row">
        <div class="col-sm-2"></div>
        <div class="col-sm-10">
            <input type=" " class="form-control" id=" " placeholder="Please enter how many minutes a Tick should equal">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-2"></div>
        <div class="col-sm-10">
            <input type=" " class="form-control" id=" " placeholder="Please enter how many Ticks the simulation should run">
        </div>
    </div>
    <!-- Tickrate -->
    <br />
    <div class="form-group row">
        <div class="col-sm-11">
            <abbr title="Back">
                <a class="btn btn-outline-primary ml-1" asp-area="" asp-controller="Home" asp-action="SimulationInfrastructure"><span class="material-icons">arrow_left</span></a>
            </abbr>
        </div>
        <div class="col-sm-1">
            <abbr title="Next">
                <a class="btn btn-outline-primary ml-1 submit" asp-area="" asp-controller="Home" asp-action="SimulationTracking"><span class="material-icons">arrow_right</span></a>
            </abbr>
        </div>
    </div>
</form>