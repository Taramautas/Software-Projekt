@{
    ViewData["Title"] = "Users";
}

<h1 class="page-title">@ViewData["Title"]</h1>

<div class="row TableHeadline">
    <p>Overview: </p>
    <div>
        <div class="row">
            <a class="btn btn-primary ml-1" asp-area="" asp-controller="Administration" asp-action="CreateUser">Create new User</a>
        </div>
        <div class="row">
            <span id="import_form_span" class="text-danger form-danger-span d-none">Please select a .json file with max. 1MB for importing booking entries.</span>
        </div>
    </div>
</div>

<table id="booking-table" style="width:100%" class="table table-hover">
    <thead class="thead-light">
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Name</th>
            <th>E-Mail</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>