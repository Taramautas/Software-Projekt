@{
    ViewData["Title"] = "Index";
}

<h2>Buchungstabelle:</h2>
<br />

<table class="table table-striped table-borderless table-hover">
    <tr>
        <th>Ladestand</th>
        <th>Fahrstrecke</th>
        <th>Startzeit</th>
        <th>Endzeit</th>
    </tr>
    @foreach (var booking in (IEnumerable<Booking>)ViewData["bookings"])
    {
        <tr>
            <td>@booking.ladestand %</td>
            <td>@booking.fahrstrecke km</td>
            <td>@booking.startzeit Uhr</td>
            <td>@booking.endzeit Uhr</td>
        </tr>
    }
</table>