@model List<A3._2.Models.Booking>
@{
    ViewData["Title"] = "Index";
}

<h1>Booking</h1>
<table class="table table-striped table-dark table-hover table-borderless">
    <thead>
        <tr>
            <th scope="col">Ladestand</th>
            <th scope="col">Nötige Distanz</th>
            <th scope="col">Beginn</th>
            <th scope="col">Ende</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var attribut in Model)
        {
        <tr>
            <th scope="row">@attribut.aktuellerLadestandDesFahrzeuges </th>
            <td>@attribut.benoetigeFahrstrecke km</td>
            <td>@attribut.startZeit</td>
            <td>@attribut.endZeit</td>
        </tr>
        }
    </tbody>
</table>

